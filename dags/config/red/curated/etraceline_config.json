        "anonymized_etraceline_d*",
        "anonymized_etraceline_l*",
        "anonymized_etraceline_mds_produit",
        "anonymized_etraceline_psl_produit",
        "anonymized_etraceline_pt_*",
        "anonymized_etraceline_s*",
        "anonymized_etraceline_t*"
