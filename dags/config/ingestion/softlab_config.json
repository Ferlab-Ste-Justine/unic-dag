{
  "concurrency": 3,
  "schedule": "0 12 * * 3",
  "timeout_hours": 4,
  "steps": [{
    "namespace": "ingestion",
    "main_class": "bio.ferlab.ui.etl.red.raw.softlab.Main",
    "publish_class": "",
    "schemas": ["softlab", "softmic"],
    "datasets":
    [
      {"dataset_id": "raw_softlab_v_p_genindex_hisaux" , "cluster_type": "small" , "run_type": "default", "cluster_specs": {}, "dependencies": []},
      {"dataset_id": "raw_softlab_v_p_lab_message"     , "cluster_type": "medium" , "run_type": "default", "cluster_specs": {}, "dependencies": ["raw_softlab_v_p_lab_test_result", "raw_softlab_v_p_lab_patient", "raw_softlab_v_p_lab_stay", "raw_softlab_v_p_lab_order"]},
      {"dataset_id": "raw_softlab_v_p_lab_order"       , "cluster_type": "small" , "run_type": "default", "cluster_specs": {}, "dependencies": ["raw_softlab_v_p_lab_stay"]},
      {"dataset_id": "raw_softlab_v_p_lab_ordered_test", "cluster_type": "small" , "run_type": "default", "cluster_specs": {}, "dependencies": []},
      {"dataset_id": "raw_softlab_v_p_lab_patient"     , "cluster_type": "small" , "run_type": "default", "cluster_specs": {}, "dependencies": ["raw_softlab_v_p_lab_stay"]},
      {"dataset_id": "raw_softlab_v_p_lab_specimen"    , "cluster_type": "small" , "run_type": "default", "cluster_specs": {}, "dependencies": ["raw_softlab_v_p_lab_order"]},
      {"dataset_id": "raw_softlab_v_p_lab_stay"        , "cluster_type": "small" , "run_type": "default", "cluster_specs": {}, "dependencies": []},
      {"dataset_id": "raw_softlab_v_p_lab_test_result" , "cluster_type": "medium", "run_type": "default", "cluster_specs": {}, "dependencies": ["raw_softlab_v_p_lab_order"]},

      {"dataset_id": "raw_softmic_v_p_mic_active_order", "cluster_type": "small" , "run_type": "default", "cluster_specs": {}, "dependencies": ["raw_softlab_v_p_lab_patient"]},
      {"dataset_id": "raw_softmic_v_p_mic_comm"        , "cluster_type": "small" , "run_type": "default", "cluster_specs": {}, "dependencies": ["raw_softmic_v_p_mic_active_order"]},
      {"dataset_id": "raw_softmic_v_p_mic_isocomm"     , "cluster_type": "small" , "run_type": "default", "cluster_specs": {}, "dependencies": ["raw_softmic_v_p_mic_isolate"]},
      {"dataset_id": "raw_softmic_v_p_mic_isolate"     , "cluster_type": "small" , "run_type": "default", "cluster_specs": {}, "dependencies": ["raw_softmic_v_p_mic_active_order"]},
      {"dataset_id": "raw_softmic_v_p_mic_media"       , "cluster_type": "small" , "run_type": "default", "cluster_specs": {}, "dependencies": ["raw_softmic_v_p_mic_test"]},
      {"dataset_id": "raw_softmic_v_p_mic_mediacomm"   , "cluster_type": "small" , "run_type": "default", "cluster_specs": {}, "dependencies": ["raw_softmic_v_p_mic_media"]},
      {"dataset_id": "raw_softmic_v_p_mic_order_comm"  , "cluster_type": "small" , "run_type": "default", "cluster_specs": {}, "dependencies": ["raw_softmic_v_p_mic_active_order"]},
      {"dataset_id": "raw_softmic_v_p_mic_sensi"       , "cluster_type": "small" , "run_type": "default", "cluster_specs": {}, "dependencies": ["raw_softmic_v_p_mic_isolate"]},
      {"dataset_id": "raw_softmic_v_p_mic_test"        , "cluster_type": "small" , "run_type": "default", "cluster_specs": {}, "dependencies": ["raw_softmic_v_p_mic_active_order"]},
      {"dataset_id": "raw_softmic_v_p_mic_testcomm"    , "cluster_type": "small" , "run_type": "default", "cluster_specs": {}, "dependencies": ["raw_softmic_v_p_mic_test"]}
    ]
  }, {
    "namespace": "anonymized",
    "main_class": "bio.ferlab.ui.etl.yellow.anonymized.Main",
    "publish_class": "",
    "schemas": [],
    "datasets":
    [
      {"dataset_id": "anonymized_softlab_v_p_genindex_hisaux", "cluster_type": "small" , "run_type": "initial", "cluster_specs": {}, "dependencies": []},
      {"dataset_id": "anonymized_softlab_v_p_lab_message"    , "cluster_type": "medium" , "run_type": "initial", "cluster_specs": {}, "dependencies": []},
      {"dataset_id": "anonymized_softlab_v_p_lab_o*"         , "cluster_type": "medium", "run_type": "initial", "cluster_specs": {}, "dependencies": []},
      {"dataset_id": "anonymized_softlab_v_p_lab_patient"    , "cluster_type": "small" , "run_type": "initial", "cluster_specs": {}, "dependencies": []},
      {"dataset_id": "anonymized_softlab_v_p_lab_s*"         , "cluster_type": "medium", "run_type": "initial", "cluster_specs": {}, "dependencies": []},
      {"dataset_id": "anonymized_softlab_v_p_lab_test_result", "cluster_type": "medium", "run_type": "default", "cluster_specs": {}, "dependencies": []},
      {"dataset_id": "anonymized_softmic_v_p_mic_*"          , "cluster_type": "medium", "run_type": "initial", "cluster_specs": {}, "dependencies": []}
    ]
  }]

}