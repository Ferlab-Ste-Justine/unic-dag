{
  "concurrency": 1,
  "schedule": "0 11 * * 6",
  "timeout_hours": 4,
  "steps": [
    {
      "namespace": "ingestion",
      "main_class": "bio.ferlab.ui.etl.red.raw.Main",
      "publish_class": "bio.ferlab.ui.etl.red.raw.UpdateLog",
      "schemas": ["chartmaxx"],
      "datasets":
      [
        {"dataset_id": "raw_chartmaxx_cmformcapture"         , "cluster_type": "medium", "run_type": "default", "cluster_specs": {}, "dependencies": []},
        {"dataset_id": "raw_chartmaxx_cmformcapture_deletes" , "cluster_type": "medium", "run_type": "default", "cluster_specs": {}, "dependencies": []}
      ]
    },
    {
      "namespace": "curated",
      "main_class": "bio.ferlab.ui.etl.red.curated.Main",
      "publish_class": "",
      "schemas": [],
      "datasets":
      [
        {"dataset_id": "curated_chartmaxx_cmformcapture", "cluster_type": "medium", "run_type": "initial", "cluster_specs": {}, "dependencies": []}
      ]
    },
    {
      "namespace": "anonymized",
      "main_class": "bio.ferlab.ui.etl.yellow.anonymized.Main",
      "publish_class": "",
      "schemas": [],
      "datasets":
      [
        {"dataset_id": "anonymized_chartmaxx_cmformcapture_obstetric_record*", "cluster_type": "small", "run_type": "initial", "cluster_specs": {}, "dependencies": []},
        {"dataset_id": "anonymized_chartmaxx_cmformcapture_deletes"          , "cluster_type": "small", "run_type": "default", "cluster_specs": {}, "dependencies": []}
      ]
    }
  ]
}
