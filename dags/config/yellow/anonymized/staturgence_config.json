{
  "concurrency": 3,
  "schedule": "0 0 * * 1",
  "timeout_hours": 5,
  "steps": [{
    "destination_zone": "yellow",
    "destination_subzone": "anonymized",
    "main_class": "bio.ferlab.ui.etl.yellow.anonymized.Main",
    "multiple_main_methods": false,
    "publish_class": "",
    "schemas": [],
    "pre_tests": [
      {"name": "greater_or_equal_partition_counts", "destinations": [
        "anonymized_staturgence_recherche_activity", "anonymized_staturgence_recherche_d*", 
        "anonymized_staturgence_recherche_episode_bcm", "anonymized_staturgence_recherche_episode_fadm",
        "anonymized_staturgence_recherche_generic", "anonymized_staturgence_recherche_location*",
        "anonymized_staturgence_recherche_patientallergy", "anonymized_staturgence_recherche_pilotage",
        "anonymized_staturgence_recherche_quest_raison_visite", "anonymized_staturgence_recherche_test"
      ]},
      {"name": "greater_partition_counts", "destinations": [
        "anonymized_staturgence_recherche_episode", "anonymized_staturgence_recherche_episode_activity", 
        "anonymized_staturgence_recherche_episode_consultation", "anonymized_staturgence_recherche_episode_diagnostic",
        "anonymized_staturgence_recherche_episode_location", "anonymized_staturgence_recherche_episode_pec",
        "anonymized_staturgence_recherche_episode_s*", "anonymized_staturgence_recherche_episode_t*",
        "anonymized_staturgence_recherche_logtransaction", "anonymized_staturgence_recherche_patientadt*"
      ]}
    ],
    "datasets": [
      {"dataset_id":"anonymized_staturgence_recherche_activity"            , "cluster_type": "small" , "run_type": "default", "cluster_specs": {}, "dependencies": []},
      {"dataset_id":"anonymized_staturgence_recherche_d*"                  , "cluster_type": "small" , "run_type": "default", "cluster_specs": {}, "dependencies": []},
      {"dataset_id":"anonymized_staturgence_recherche_episode*"             , "cluster_type": "medium", "run_type": "default", "cluster_specs": {}, "dependencies": []},
      {"dataset_id":"anonymized_staturgence_recherche_generic"             , "cluster_type": "small" , "run_type": "default", "cluster_specs": {}, "dependencies": []},
      {"dataset_id":"anonymized_staturgence_recherche_location*"           , "cluster_type": "small" , "run_type": "default", "cluster_specs": {}, "dependencies": []},
      {"dataset_id":"anonymized_staturgence_recherche_logtransaction"      , "cluster_type": "small" , "run_type": "default", "cluster_specs": {}, "dependencies": []},
      {"dataset_id":"anonymized_staturgence_recherche_patient*"         , "cluster_type": "small" , "run_type": "default", "cluster_specs": {}, "dependencies": []},
      {"dataset_id":"anonymized_staturgence_recherche_pilotage"            , "cluster_type": "small" , "run_type": "default", "cluster_specs": {}, "dependencies": []},
      {"dataset_id":"anonymized_staturgence_recherche_quest_raison_visite" , "cluster_type": "small" , "run_type": "default", "cluster_specs": {}, "dependencies": []},
      {"dataset_id":"anonymized_staturgence_recherche_test"                , "cluster_type": "small" , "run_type": "default", "cluster_specs": {}, "dependencies": []}
    ],
    "post_tests": [
      {"name": "lower_or_equal_null_counts", "destinations": ["anonymized_staturgence_*"]},
      {"name": "equal_counts", "destinations": ["anonymized_staturgence_*"]}
    ]
  }]
}