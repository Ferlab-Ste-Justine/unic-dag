{
  "concurrency": 3,
  "schedule": "0 0 * * 3",
  "timeout_hours": 6,
  "steps": [
    {
      "destination_zone": "red",
      "destination_subzone": "curated",
      "main_class": "bio.ferlab.ui.etl.red.curated.pharmacie.Main",
      "multiple_main_methods": false,
      "publish_class": "",
      "schemas": [],
      "pre_tests": [],
      "datasets": [
        {"dataset_id":"curated_pharmacie_g*"   , "cluster_type": "small" , "run_type": "default", "cluster_specs": {}, "dependencies": []},
        {"dataset_id":"curated_pharmacie_phf*" , "cluster_type": "medium", "run_type": "default", "cluster_specs": {}, "dependencies": []}
      ],
      "optimize" : [],
      "post_tests": []
    },
    {
