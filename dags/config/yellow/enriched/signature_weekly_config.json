{
  "concurrency": 2,
  "start_date": "2025-11-14",
  "schedule": "0 6 * * 5",
  "catchup": true,
  "timeout_hours": 4,
  "steps": [
    {
      "destination_zone": "yellow",
      "destination_subzone": "enriched",
      "main_class": "bio.ferlab.ui.etl.yellow.enriched.signature.Main",
